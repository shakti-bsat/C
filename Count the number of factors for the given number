#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,count=0;
    scanf("%d",&a);
    for(int i=1;i<=a/2;i++){
        if(a%i==0){
        count = count+1;
        }
    }
    printf("%d",count+1);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
