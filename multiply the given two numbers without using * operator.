#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,b,sum =0;
    scanf("%d %d",&a,&b);
    for(int i=0;i<a;i++){
        sum = sum+b;
    }
    printf("%d",sum);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
