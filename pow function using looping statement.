#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int base,power,mul =1;
    scanf("%d %d",&base,&power);
    for(int i=1;i<=power;i++){
        mul = mul*base;
    }
    printf("%d",mul);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
