#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,b;
    scanf("%d %d",&a,&b);
    for(int i=1;i<=a;i++){
        printf("%d * %d = %d\n",i,b,i*b);
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
