#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,sum=0;
    scanf("%d",&a);
    for(int i = 1;i<=a;i++){
        if(a%i==0){
            sum = sum+i;
        }
    }
    printf("%d",sum);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}


