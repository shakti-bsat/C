#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char a;
    scanf("%c",&a);
    int b = a;
    if(b>=65 && b<=90){
        printf("UPPERCASE");
    }
    else{
        printf("LOWERCASE");
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
